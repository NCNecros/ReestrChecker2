            <plugin>
                <groupId>org.pitest</groupId>
                <artifactId>pitest-maven</artifactId>
                <version>1.1.4</version>
                <configuration>
                    <targetClasses>
                        <param>company.*</param>
                    </targetClasses>
                    <targetTests><param>company.*</param></targetTests>
                </configuration>
            </plugin>
        </plugins>
    </reporting>

